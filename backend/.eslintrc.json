{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 13,
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "semi": ["warn", "always"],
    "quotes": ["warn", "double"],
    "no-console": "off",
    "import/no-unresolved": "off",
    "import/extensions": "off",
    "comma-dangle": "off",
    "no-unused-vars": "off",
    "object-curly-newline": "off"
  }
}
